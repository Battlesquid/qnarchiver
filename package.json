{
    "name": "vex-qna-archiver",
    "author": "battlesqui_d",
    "description": "Utility package to scrape questions from the VEX Robotics Q&A.",
    "version": "8.0.1",
    "license": "GPL-3.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/Battlesquid/vex-qna-archiver.git"
    },
    "keywords": [
        "vex",
        "vex robotics",
        "qna",
        "scrape"
    ],
    "main": "dist/src/index.js",
    "scripts": {
        "test": "jest",
        "clean": "rm -rf dist/",
        "build": "yarn tsc",
        "lint": "eslint . --ext .ts",
        "linkbuild": "yarn build && yarn link",
        "relink": "yarn unlink && yarn linkbuild",
        "prepare": "husky install"
    },
    "files": [
        "dist/src/**/**"
    ],
    "dependencies": {
        "@crawlee/core": "^3.7.2",
        "cheerio": "^1.0.0-rc.3",
        "got-scraping": "^4.0.3",
        "pino": "^8.17.2"
    },
    "engines": {
        "node": "20.11.0"
    },
    "devDependencies": {
        "@types/cheerio": "^0.22.22",
        "@types/jest": "^29.5.3",
        "@types/node": "^20.4.8",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "eslint": "^8.7.0",
        "eslint-plugin-sonarjs": "^0.20.0",
        "husky": "^8.0.3",
        "jest": "^29.6.2",
        "lint-staged": "^13.2.3",
        "pino-pretty": "^10.2.0",
        "prettier": "3.0.1",
        "release-it": "^17.0.3",
        "ts-jest": "^29.1.1",
        "typescript": "^5.1.6"
    },
    "types": "dist/src/index.d.ts",
    "lint-staged": {
        "*.ts": "yarn lint --fix",
        "**/*": "prettier --write --ignore-unknown"
    },
    "volta": {
        "node": "20.11.0",
        "yarn": "1.22.19",
        "npm": "10.3.0"
    }
}
